{
  "extends": ["config:base"],
  "dependencyDashboard": false,
  "assignees": ["siakhooi"],
  "reviewers": ["siakhooi"],
  "dockerfile": {
    "enabled": false
  },
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["Dockerfile"],
      "matchStrings": [
        "ARG JQ_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "jqlang/jq",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "fileMatch": ["Dockerfile"],
      "matchStrings": [
        "ARG YQ_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "mikefarah/yq",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "fileMatch": ["Dockerfile"],
      "matchStrings": [
        "ARG SHFMT_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "mvdan/sh",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "fileMatch": ["Dockerfile"],
      "matchStrings": [
        "ARG NVM_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "nvm-sh/nvm",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "fileMatch": ["^go125/Dockerfile$"],
      "matchStrings": [
        "ARG GO_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "golang",
      "datasourceTemplate": "golang-version"
    },
    {
      "customType": "regex",
      "fileMatch": ["^go125/Dockerfile$"],
      "matchStrings": [
        "ARG GOLANGCI_LINT_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "golangci/golangci-lint",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "fileMatch": ["^go125/Dockerfile$"],
      "matchStrings": [
        "ARG GORELEASER_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "goreleaser/goreleaser",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "fileMatch": ["^minizinc/Dockerfile$"],
      "matchStrings": [
        "ARG MINIZINC_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "MiniZinc/MiniZincIDE",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "fileMatch": ["^jdk.*/Dockerfile$"],
      "matchStrings": [
        "ARG GRADLE_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "gradle",
      "datasourceTemplate": "gradle-version"
    },
    {
      "customType": "regex",
      "fileMatch": ["^jdk.*/Dockerfile$"],
      "matchStrings": [
        "ARG SPRING_BOOT_CLI_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "org.springframework.boot:spring-boot-cli",
      "datasourceTemplate": "maven"
    },
    {
      "customType": "regex",
      "fileMatch": ["^jdk.*/Dockerfile$"],
      "matchStrings": [
        "ARG MAVEN_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "org.apache.maven:apache-maven",
      "datasourceTemplate": "maven"
    },
    {
      "customType": "regex",
      "fileMatch": ["^jdk.*/Dockerfile$"],
      "matchStrings": [
        "ARG GROOVY_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "org.apache.groovy:groovy-all",
      "datasourceTemplate": "maven"
    }
  ],
  "packageRules": [
    {
      "matchDepNames": ["golang"],
      "allowedVersions": "~1.25"
    }
  ]
}