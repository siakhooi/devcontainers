{
  "extends": ["config:base"],
  "dependencyDashboard": false,
  "assignees": ["@me"],
  "reviewers": ["@me"],
  "dockerfile": {
    "enabled": false
  },
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["Dockerfile"],
      "matchStrings": [
        "ARG JQ_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "jqlang/jq",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "fileMatch": ["Dockerfile"],
      "matchStrings": [
        "ARG YQ_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "mikefarah/yq",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "fileMatch": ["^go125/Dockerfile$"],
      "matchStrings": [
        "ARG GO_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "golang/go",
      "datasourceTemplate": "github-releases",
      "versioningTemplate": "semver"
    },
    {
      "customType": "regex",
      "fileMatch": ["^go125/Dockerfile$"],
      "matchStrings": [
        "ARG GOLANGCI_LINT_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "golangci/golangci-lint",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "fileMatch": ["^go125/Dockerfile$"],
      "matchStrings": [
        "ARG GORELEASER_VERSION=(?<currentValue>[0-9.]+)"
      ],
      "depNameTemplate": "goreleaser/goreleaser",
      "datasourceTemplate": "github-releases"
    }
  ],
  "packageRules": [
    {
      "matchDepNames": ["golang/go"],
      "allowedVersions": "^1\\.25"
    }
  ]
}