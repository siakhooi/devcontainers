{
	"name": "Python Development Dev Container",
	"image": "siakhooi/devcontainer:python-0.1.0",
	"remoteUser": "maui",
	"containerUser": "maui",
	"updateRemoteUserUID": true,
	"features": {},
	"customizations": {
		"vscode": {
			"extensions": [
				"github.copilot-chat",
				"ms-python.python",
				"ms-python.vscode-pylance"
			]
		}
	},
	"mounts": [
		"source=${localEnv:HOME}${localEnv:USERPROFILE}/.ssh,target=/home/maui/.ssh,type=bind,consistency=cached"
	],
	"remoteEnv": {
		"LOCAL_WORKSPACE_FOLDER": "${localWorkspaceFolder}"
	}
}
